{
  "name": "pulse-backend",
  "version": "0.0.1",
  "engineStrict": true,
  "engines": {
    "node": "8.10"
  },
  "main": "index.js",
  "scripts": {
    "gulp": "gulp",
    "test": "./node_modules/.bin/mocha --timeout 30000 --compilers js:@babel/register $(find ./src \\( -name '*.unit.js' -o -name '*.spec.js' \\) -not -path './node_modules/*')",
    "watch": "NODE_ENV=test ./node_modules/.bin/mocha -r @babel/register -w $(find ./src \\( -name '*.unit.js' -o -name '*.spec.js' \\) -not -path './node_modules/*')",
    "test-i": "./node_modules/.bin/mocha --compilers js:@babel/register src/test/*.integration.js",
    "offline": "cd build && sls offline --stage dev --host $(ipconfig getifaddr en0)",
    "localhost-run": "cd build && sls offline --stage dev",
    "integration": "concurrently --kill-others --raw --success first \"npm run localhost-run\" \"wait-on tcp:localhost:3000 && npm run test-i\"",
    "lint": "./node_modules/.bin/eslint --fix './**/!(*.spec).js' && ./node_modules/.bin/eslint './**/!(*.spec).js'",
    "prepush": "npm test && npm run lint",
    "coverage": "./node_modules/.bin/nyc npm test && \"./node_modules/.bin/nyc\" report --reporter=text-lcov > coverage.lcov",
    "release": "standard-version"
  },
  "keywords": [
    "api"
  ],
  "author": "Marc Fielding",
  "license": "private",
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-transform-async-to-generator": "^7.1.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "aws-sdk-mock": "^2.0.0",
    "babel-plugin-istanbul": "^5.1.1",
    "chai": "^4.1.2",
    "concurrently": "^3.1.0",
    "cz-conventional-changelog": "^1.2.0",
    "eslint": "^3.14.1",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-plugin-import": "^2.14.0",
    "gulp": "^4.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-cli": "github:gulpjs/gulp-cli",
    "gulp-concat": "^2.6.1",
    "gulp-insert": "^0.5.0",
    "gulp-order": "^1.1.1",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-string-replace": "^0.4.0",
    "husky": "^0.13.1",
    "merge-stream": "^1.0.1",
    "mocha": "^5.1.1",
    "mocha.parallel": "^0.15.5",
    "nock": "^9.2.5",
    "nyc": "^10.1.2",
    "require-dir": "^0.3.1",
    "serverless-delete-loggroups": "^0.3.1",
    "serverless-offline": "^3.33.0",
    "serverless-offline-sns": "^0.53.0",
    "serverless-plugin-aws-alerts": "^1.2.4",
    "serverless-plugin-canary-deployments": "^0.4.5",
    "serverless-prune-plugin": "^1.3.2",
    "sinon": "^5.0.3",
    "sinon-chai": "^3.0.0",
    "standard-version": "^4.0.0",
    "supertest": "^2.0.1",
    "swagger-parser": "^3.4.1",
    "wait-on": "^2.0.1",
    "yamljs": "^0.2.8",
    "yargs": "^6.5.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.1.2",
    "aws-sdk": "^2.36.0",
    "axios": "^0.18.0",
    "babel-runtime": "^6.23.0",
    "bluebird": "^3.5.0",
    "bunyan": "^1.8.12",
    "config": "^1.25.1",
    "elastic-apm-node": "^2.3.0",
    "elasticsearch": "^14.0.0",
    "faker": "^4.1.0",
    "fs-extra": "^7.0.1",
    "install": "^0.8.7",
    "ip-range-check": "0.0.2",
    "joi": "^10.2.2",
    "jsonwebtoken": "^7.4.3",
    "lodash": "^4.17.11",
    "lodash._basecreate": "^3.0.3",
    "moment": "^2.22.1",
    "mysql": "^2.13.0",
    "npm": "^5.6.0",
    "promise-mysql": "^3.3.1",
    "ramda": "^0.25.0",
    "raven": "^2.1.2",
    "regenerator-runtime": "^0.10.5",
    "request": "^2.81.0",
    "serverless-plugin-split-stacks": "^1.7.3",
    "serverless-pseudo-parameters": "^2.4.0",
    "unzipper": "^0.10.0",
    "urlsafe-base64": "^1.0.0",
    "uuid": "^3.3.2",
    "winston": "^2.4.0",
    "zip-folder": "^1.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
